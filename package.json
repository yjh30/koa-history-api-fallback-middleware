  "homepage": "https://github.com/yjh30/koa-history-api-fallback-middleware#readme",
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "^2.1.0",
    "ghooks": "^2.0.4",
    "lint-staged": "^8.1.0",
    "validate-commit-msg": "^2.14.0"
